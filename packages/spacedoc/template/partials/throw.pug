//- Render the exceptions a doclet can throw.
//- @param {Object[]} throws - Exceptions.
mixin throws(throws)
  if throws && throws.length
    section
      h6 Throws
      ul
        each thr in throws
          if thr.types
            li <code>#{thr.types.join(', ')}</code> #{spacedoc.markdown(thr.description)}
          else
            li!= spacedoc.markdown(thr.description, true)
