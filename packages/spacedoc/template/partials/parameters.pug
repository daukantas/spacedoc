mixin parameters(params)
  if params && params.length
    section
      h6= Parameters
      table
        thead: tr
          td Name
          td Type
          td Default
          td Description
        tbody
          each param in params
            tr
              td= param.name
              td!= param.types.join(', ')
              td
                if param.default
                  code= param.default
              td!= helpers.markdown(param.description, true)
