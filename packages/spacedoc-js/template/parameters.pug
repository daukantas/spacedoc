mixin parameters(params)
  if params && params.length
    table
      thead
        tr
          th Name
          th Type
          th Default
          th Description
      tbody
        each param in params
          tr
            td= param.name
            td= helpers.type(param.type)
            td
              if param.defaultvalue
                code= param.defaultvalue
            td!= helpers.markdown(param.description)
